<html>
	<Head>
		<title>SAT</title>
		<link rel="stylesheet" type="text/css" href="css\bootstrap.css">
		<link rel="stylesheet" type="text/css" href="css\Style.css">
	</Head>
	<body>
		<form method="post" action="LoginFrm.aspx">
			<table align="center" style="margin-top:100px">
				<tr>
					<td>
						<img src="Icon\Logo-black-blue.png" height="250px" width="250px">
					</td>
				</tr>
				<table class="my-table" align="center">
					<tr>
						<td>
						<input class="form-control" type="text" name="loginTxtBox" placeholder="User">
						</td>
					</tr>
					<tr>
						<td>
						<input class="form-control" type="text" name="emailTxtBox" placeholder="E-mail">
						</td>
					</tr>
					<tr>
						<td>
						<input class="btn btn-lg btn-primary btn-block" type="submit" name="submitBtn" value="Send new password">
						</td>
					</tr>
				</table>
			</table>
		</form>
	</body>
</html>